{
  "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
  "tasks.max": "1",
  "database.hostname": "postgres",
  "database.port": "5432",
  "database.user": "books-svc",
  "database.password": "secret",
  "database.dbname" : "booksdb",
  "topic.prefix": "acme.",
  "schema.include.list": "public",
  "table.include.list" : "public.auditlog",
  "tombstones.on.delete" : "false",
  "transforms" : "outbox",
  "transforms.outbox.type" : "io.debezium.transforms.outbox.EventRouter",
  "transforms.outbox.route.topic.replacement" : "auditlog",
  "transforms.outbox.route.by.field" : "entity_type",
  "transforms.outbox.table.expand.json.payload": "true",
  "transforms.outbox.table.field.event.key": "id",
  "transforms.outbox.table.field.event.timestamp": "created",
  "transforms.outbox.table.fields.additional.placement" : "entity_type:header:entityType,event_type:header:eventType",
  "value.converter": "org.apache.kafka.connect.json.JsonConverter",
  "plugin.name": "pgoutput",
  "time.precision.mode": "adaptive"
}